package controllers;

import models.Game;
import view.TUI;

public class GameController {
	private Game game;
	private TUI ui;
	
	public GameController(){
		game = new Game();
		ui = new MenuTUI();
	}
	public static void main(String[] args){
		GameController controller = new GameController();
		controller.start();
	}
	
	public void start(){
		ui.start();
	}
}
